{
  "name": "vscode-hydra",
  "version": "0.0.1",
  "displayName": "Hydra",
  "description": "Hydra video synthesizer for Visual Studio Code",
  "publisher": "jdomizz",
  "license": "AGPL-3.0-or-later",
  "main": "./out/extension.js",
  "icon": "media/icon.png",
  "engines": {
    "vscode": "^1.88.0"
  },
  "activationEvents": [],
  "contributes": {
    "commands": [
      {
        "command": "vscode-hydra.createPanel",
        "title": "Create a panel",
        "category": "Hydra"
      }
    ]
  },
  "scripts": {
    "compile": "npm run compile:backend && npm run compile:frontend",
    "compile:backend": "tsc",
    "compile:frontend": "rollup -c",
    "watch": "tsc -watch",
    "lint": "eslint src",
    "pretest": "npm run compile && npm run lint",
    "test": "vscode-test",
    "vscode:prepublish": "npm run compile"
  },
  "dependencies": {
    "hydra-synth": "^1.3.29"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.88.0",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "@vscode/test-cli": "^0.0.8",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.57.0",
    "rollup": "^4.14.1",
    "typescript": "^5.3.3"
  },
  "categories": [
    "Programming Languages",
    "Visualization"
  ],
  "keywords": [
    "hydra-synth",
    "creative-coding",
    "livecoding"
  ]
}
